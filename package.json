{
  "name": "unplugin-require-transform",
  "version": "0.0.7",
  "packageManager": "pnpm@7.13.6",
  "description": "transform require into import",
  "keywords": [
    "unplugin",
    "require-transform",
    "unplugin-require-transform"
  ],
  "license": "MIT",
  "author": "tangdaoyuan <1197633750@qq.com> (https://github.com/tangdaoyuan/)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tangdaoyuan/unplugin-require-transform.git"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "./vite": {
      "require": {
        "types": "./dist/vite.d.ts",
        "default": "./dist/vite.cjs"
      },
      "import": {
        "types": "./dist/vite.d.ts",
        "default": "./dist/vite.mjs"
      }
    },
    "./types": {
      "require": "./dist/types.cjs",
      "import": "./dist/types.mjs",
      "types": "./dist/types.d.ts"
    },
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./*": "./*"
  },
  "files": [
    "dist",
    "*.d.ts",
    "@types"
  ],
  "scripts": {
    "test": "vitest",
    "test:run": "vitest run",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "node ./src/index.ts",
    "play": "npm -C playground run dev",
    "stub": "unbuild --stub",
    "build": "unbuild",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp --tag --commit --push && pnpm publish"
  },
  "bugs": {
    "url": "https://github.com/tangdaoyuan/unplugin-require-transform/issues"
  },
  "homepage": "https://github.com/tangdaoyuan/unplugin-require-transform#readme",
  "peerDependencies": {
    "esbuild": "*",
    "rollup": "*",
    "vite": "*"
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "magic-string": "^0.26.2",
    "picocolors": "^1.0.0",
    "unplugin": "^1.0.0"
  },
  "devDependencies": {
    "@suger-tdy/eslint-config": "1.1.12",
    "bumpp": "8.2.1",
    "esbuild": "0.15.12",
    "eslint": "8.26.0",
    "typescript": "4.8.4",
    "unbuild": "0.9.4",
    "vite": "3.1.8",
    "vitest": "0.24.3"
  }
}
