{
  "name": "unplugin-require-transform",
  "version": "0.1.3",
  "packageManager": "pnpm@7.1.1",
  "description": "keep polite by plugin",
  "keywords": [
    "unplugin",
    "require-transform",
    "unplugin-require-transform"
  ],
  "license": "MIT",
  "author": "tangdaoyuan <1197633750@qq.com> (https://github.com/tangdaoyuan/)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tangdaoyuan/unplugin-require-transform.git"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "./vite": {
      "require": {
        "types": "./dist/vite.d.ts",
        "default": "./dist/vite.cjs"
      },
      "import": {
        "types": "./dist/vite.d.ts",
        "default": "./dist/vite.mjs"
      }
    },
    "./types": {
      "require": "./dist/types.cjs",
      "import": "./dist/types.mjs",
      "types": "./dist/types.d.ts"
    },
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./*": "./*"
  },
  "files": [
    "dist",
    "*.d.ts",
    "@types"
  ],
  "scripts": {
    "test": "vitest",
    "test:run": "vitest run",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "node ./src/index.ts",
    "play": "npm -C playground run dev",
    "stub": "unbuild --stub",
    "build": "unbuild",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp --tag --commit --push && pnpm publish"
  },
  "bugs": {
    "url": "https://github.com/tangdaoyuan/unplugin-require-transform/issues"
  },
  "homepage": "https://github.com/tangdaoyuan/unplugin-require-transform#readme",
  "peerDependencies": {
    "esbuild": "*",
    "rollup": "*",
    "vite": "*"
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "chokidar": "^3.5.3",
    "fast-glob": "^3.2.11",
    "import-module-runtime": "^0.1.0",
    "magic-string": "^0.26.2",
    "picocolors": "^1.0.0",
    "regexparam": "^2.0.0",
    "unplugin": "^0.6.3"
  },
  "devDependencies": {
    "@suger-tdy/eslint-config": "^1.1.0",
    "bumpp": "^7.1.1",
    "esbuild": "^0.14.42",
    "eslint": "^8.16.0",
    "typescript": "^4.7.2",
    "unbuild": "^0.7.4",
    "vite": "^2.9.9",
    "vitest": "^0.9.4"
  }
}
